    <h1>Compose</h1>
    <form class="" action="/updatePage" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" value="<%=post.title%>" name="postTitle">
        <label>Post</label>
        <textarea class="form-control" name="postBody" value="<%=post.content%>" rows="5" cols="30"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Update</button>
    </form>